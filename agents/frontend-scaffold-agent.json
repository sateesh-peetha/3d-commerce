{
    "agent_id": "frontend-scaffold-agent",
    "version": "1.0.0",
    "spec_hash": "3DC-SPEC-2026-02-04-a1b2c3d4e5f6",
    "objective": "Convert block compositions into component skeletons",
    "input_specs": [
        "/specs/ui-blocks.json",
        "/specs/design-tokens.json",
        "/outputs/compositions/*.json"
    ],
    "allowed_outputs": [
        "react_component_stubs",
        "react_native_component_stubs",
        "props_interfaces",
        "layout_containers",
        "typescript_types"
    ],
    "forbidden_outputs": [
        "business_logic",
        "api_calls",
        "state_management",
        "inline_styles"
    ],
    "output_path": "/outputs/frontend/",
    "output_format": {
        "components": "components/{BlockName}/{BlockName}.tsx",
        "types": "types/{BlockName}.types.ts",
        "styles": "components/{BlockName}/{BlockName}.module.css"
    },
    "validation_rules": [
        "no_inline_styles",
        "token_references_only",
        "typescript_strict",
        "components_compile"
    ],
    "exit_condition": "All components compile without errors"
}